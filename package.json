{
  "name": "dist-zip-plugin",
  "version": "1.0.0",
  "description": "dist zip plugin",
  "main": "./dist/index.js",
  "type": "module",
  "scripts": {
    "build": "rollup -c",
    "prepublish": "npm run build",
    "publish": "npm publish --registry https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "https://gitee.com/flaw_du/build-pugin.git"
  },
  "files": [
    "dist"
  ],
  "exclude": [
    "node_modules"
  ],
  "keywords": [
    "webpack",
    "dist",
    "zip"
  ],
  "engines": {
    "node": ">=14"
  },
  "author": "sugar_free",
  "license": "ISC",
